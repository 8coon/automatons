{
  "name": "automatons",
  "version": "0.0.4",
  "description": "Side effect management library based on finite state machines",
  "main": "dist/automatons.cjs.js",
  "scripts": {
    "test": "jest --coverage",
    "build": "rm -rf lib/ && tsc && rollup --config rollup.config.js",
    "lint": "tslint -c tslint.json 'src/**/*.ts'",
    "fix": "npm run lint -- --fix",
    "docs": "typedoc --name Automatons --readme none --theme minimal --out docs/ --mode file src/",
    "prepublishOnly": "npm run lint && npm run build && npm test"
  },
  "keywords": [
    "side-effect",
    "state-management",
    "react",
    "finite-state-machine",
    "fsm"
  ],
  "author": "Kaibito Young",
  "license": "MIT",
  "repository": "https://github.com/8coon/automatons",
  "files": [
    "dist",
    "lib"
  ],
  "types": "lib/index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint",
      "pre-push": "npm run build && npm test"
    }
  },
  "devDependencies": {
    "@types/enzyme": "^3.1.15",
    "@types/enzyme-adapter-react-16": "^1.0.3",
    "@types/jest": "^23.3.12",
    "@types/react": "^16.7.20",
    "@types/react-test-renderer": "^16.0.3",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "rollup": "^1.1.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "ts-jest": "^23.10.5",
    "tslib": "^1.9.3",
    "tslint": "^5.12.1",
    "typedoc": "^0.14.2",
    "typescript": "^3.2.2"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "dependencies": {}
}
